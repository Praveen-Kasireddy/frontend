<div class="_submenu">
    <div class="_dimension-selector"></div>

    <button class="btn btn-default _back" style="display: none">
        <i class="fa fa-chevron-left"></i>
        Back
    </button>

    <div class="_filterByState input-group" style="display: none">
	            <span class="input-group-addon">
	                <i class="fa fa-filter"></i>
	            </span>
        <select class="form-control">
            <option value="all">All sets</option>
            <option value="valid">Valid sets</option>
            <option value="invalid" selected>Invalid sets</option>
        </select>
    </div>

    <button class="btn btn-default _auto-reconcile" style="display: none">
        <i class="fa fa-magic"></i>
        Auto-reconcile selected sets...
    </button>

    <!-- Modal -->
    <div class="modal" tabindex="-1" role="dialog" data-backdrop="static"
         data-keyboard="false">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Auto-reconcile</h5>
                </div>
                <div class="modal-body">
                    Move delta inside the following:
                    <div class="_list-of-inputs">

                    </div>
                    <label>
                        <input type="radio" value="_new" name="attribute">
                        New child:
                        <input type="text" class="form-control" name="new_attribute">
                    </label>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-primary _apply">Apply</button>
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="_dataStructuresTable"></div>

<div class="_setsTable" style="display: none"></div>
