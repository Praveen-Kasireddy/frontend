{
    "name": "kpmg-kosmos",
    "version": "0.0.69",
    "description": "KPMG KOSMOS web frontend",
    "scripts": {
        "ng": "ng",
        "ng-proxy": "ng serve --proxy-config proxyconfig.json",
        "start": "ng serve --common-chunk=false --vendor-chunk=false --open=true",
        "start:normal": "ng serve",
        "start:optimized": "ng serve --env=dev --dev --aot --vc --cc --dop --build-optimizer=true",
        "start:aot": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng serve --aot",
        "start:aot:de": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng serve --aot --configuration=de-dev",
        "build": "yarn run build:renderer-css && yarn run build:normal",
        "build:normal": "ng build --prod --aot=false --build-optimizer=false",
        "build:optimized": "ng build --prod --aot --build-optimizer=true --vendor-chunk=true --common-chunk=true --delete-output-path=true",
        "build:dev:optimized": "ng build --dev --aot --vc --cc --dop --buildOptimizer",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "test": "ng test --source-map=false --browsers CH",
        "test:watch": "ng test --watch=true --source-map=false --browsers Chrome",
        "test:debug": "ng test --watch=true --source-map=false --browsers ChromeDebug",
        "i18n:export": "ng xi18n  --i18n-format=xlf2 --out-file locale/messages.xlf",
        "generate:exports": "node tooling/generate-index-exports",
        "generate:version": "node tooling/generate-version",
        "build:renderer-css": "node-sass --include-path --output-style compressed src/assets/styles/renderer/renderer.scss src/assets/styles/renderer.css"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^6.1.10",
        "@angular/cdk": "^6.4.7",
        "@angular/common": "^6.1.10",
        "@angular/compiler": "^6.1.10",
        "@angular/core": "^6.1.10",
        "@angular/forms": "^6.1.10",
        "@angular/http": "^6.1.10",
        "@angular/material": "^6.4.7",
        "@angular/platform-browser": "^6.1.10",
        "@angular/platform-browser-dynamic": "^6.1.10",
        "@angular/router": "^6.1.10",
        "@ng-bootstrap/ng-bootstrap": "^2.2.2",
        "@ngx-progressbar/core": "^5.2.0",
        "@ngx-progressbar/http": "^5.2.0",
        "@types/file-saver": "^1.3.0",
        "@types/jszip": "^3.1.4",
        "@xbs/filemanager": "^5.4.2",
        "@xbs/spreadsheet": "^5.4.0",
        "@xbs/webix-pro": "^5.4.3",
        "angular-cropperjs": "^1.0.1",
        "angular-froala-wysiwyg": "^2.8.4",
        "bootstrap": "4.1.3",
        "bootstrap-sass": "^3.3.7",
        "bootstrap-slider": "^10.2.0",
        "browserify-zlib": "^0.2.0",
        "chroma-js": "^1.3.7",
        "cldr-data": "^32.0.1",
        "core-js": "^2.5.7",
        "cropperjs": "^1.4.1",
        "devextreme": "^18.1.6",
        "devextreme-angular": "^18.1.6",
        "file-saver": "^1.3.8",
        "flag-icon-css": "^3.0.0",
        "font-awesome": "^4.7.0",
        "globalize": "^1.4.0",
        "highcharts": "^5.0.14",
        "intl": "^1.2.5",
        "jasmine-tfs-reporter": "^1.0.2",
        "jquery": "^3.2.1",
        "jquery-contextmenu": "^3.0.0-beta.2",
        "jquery-ui": "^1.12.1",
        "jquery-ui-dist": "^1.12.1",
        "json-loader": "^0.5.7",
        "jszip": "^3.1.5",
        "lodash": "^4.17.10",
        "mathjs": "^7.5.1",
        "moment": "^2.22.2",
        "ngx-logger": "^3.0.5",
        "ngx-store": "^2.0.0",
        "ngx-take-until-destroy": "^3.0.0",
        "normalize.css": "^8.0.0",
        "popper.js": "^1.14.4",
        "rxjs": "^6.2.2",
        "sortablejs": "^1.6.1",
        "stream": "^0.0.2",
        "tooltip.js": "^1.3.0",
        "vis": "^4.21.0",
        "web-animations-js": "^2.3.1",
        "xlsx": "^0.11.5",
        "xlsx-style": "^0.8.13",
        "zone.js": "^0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.8.5",
        "@angular-devkit/build-optimizer": "^0.8.5",
        "@angular/cli": "^6.2.5",
        "@angular/compiler-cli": "^6.1.10",
        "@angular/language-service": "^6.1.10",
        "@types/jasmine": "~2.8.7",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~10.5.6",
        "codelyzer": "^4.4.3",
        "jasmine-core": "~3.2.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~2.0.5",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "^2.0.1",
        "karma-jasmine": "~1.1.2",
        "karma-jasmine-html-reporter": "^1.2.0",
        "node-sass": "^4.9.2",
        "protractor": "~5.4.0",
        "ts-node": "~7.0.1",
        "tslint": "~5.11.0",
        "tslint-angular": "^1.1.2",
        "typescript": "2.9.2",
        "gulp": "^3.9.1",
        "gulp-compile-handlebars": "^0.6.1",
        "gulp-concat": "*",
        "gulp-rename": "^1.3.0",
        "gulp-sass": "^4.0.2",
        "gulp-sourcemaps": "*",
        "gulp-uglify": "*",
        "gulp-util": "*",
        "gulp-watch": "^4.3.9",
        "lodash": "^4.17.10",
        "webpack-stream": "^4.0.3"
    }
}
